<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diplom.app.fitnessproject">

    <application
        android:allowBackup="true"
        android:isGame="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app.name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity
            android:screenOrientation="portrait"
            android:name=".view.MainActivity"
            android:label="@string/title.activity.main"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.UprazneniaActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".view.UprazneniaAddActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".view.UprazneniaActivity" />
        </activity>
        <activity android:name=".view.activity.UprazneniaAddCustomChooseCategory"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".view.UprazneniaAddActivity" />
        </activity>
        <activity android:name=".view.UprazneniaAddComplex">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".view.UprazneniaActivity" />
        </activity>
        <activity android:name=".view.activity.UprazneniaAllList"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".view.UprazneniaAddComplex" />
        </activity>
        <activity android:name=".view.TrainingsActivity">

        </activity>
    </application>

</manifest>
